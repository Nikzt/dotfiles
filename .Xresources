! URxvt.colorIT:      	#a3be8c
! URxvt.colorBD:      	#e5e9f0
! URxvt.colorUL:      	#81a1c1
URxvt.geometry: 		80x40
URxvt.internalBorder:	9
URxvt*letterSpace: 	    0
URxvt*lineSpace: 		0
URxvt*font:             xft:Iosevka Term:regular:size=11
