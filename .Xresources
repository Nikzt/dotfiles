! ------------------------------------------------------------------------------
! Nord Color Scheme
! By Arctic Ice Studio 
! ------------------------------------------------------------------------------

! special
*.foreground:   #d8dee8
*.background:   #2f343f
*.cursorColor:  #b48ead

! black
*.color0  : #4b5262
*.color8  : #434a5a

! red
*.color1  : #bf616a
*.color9  : #b3555e

! green
*.color2  : #a3be8c
*.color10 : #93ae7c

! yellow
*.color3  : #ebcb8b
*.color11 : #dbbb7b

! blue
*.color4  : #81a1c1
*.color12 : #7191b1

! magenta
*.color5  : #b48ead
*.color13 : #a6809f

! cyan
*.color6  : #93e5cc
*.color14 : #7bcdb4

! white
*.color7  : #e5e9f0
*.color15 : #d1d5dc

URxvt*iso14755:         false
URxvt*depth:            32
URxvt.underlineColor:   #81a1c1
URxvt.highlightColor:   #7191b1
URxvt.throughColor:     #93e5cc
URxvt*cursorColor:      #b48ead
URxvt*cursorColor2:     #d8dee8
URxvt*scrollBar:        false
URxvt*scrollBar_right:  false
URxvt*internalBorder:   0
URxvt*externalBorder:   0
URxvt.colorIT:      	#a3be8c
URxvt.colorBD:      	#e5e9f0
URxvt.colorUL:      	#81a1c1
URxvt.geometry: 		92x24
URxvt.internalBorder:	24
URxvt*letterSpace: 	    0
URxvt*lineSpace: 		-3
URxvt*font:             xft:M+ 1MN:regular:size=8
URxvt*imFont:           xft:M+ 1MN:regular:size=8
URxvt*boldFont:         xft:M+ 1MN:bold:size=8
URxvt*italicFont:       xft:M+ 1MN:italic:size=8
URxvt*boldItalicFont:   xft:M+ 1MN:regular:size=8

! ------------------------------------------------------------------------------
! Xft Font Configuration
! ------------------------------------------------------------------------------

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 0
Xft.antialias: 1
Xft.rgba: rgb
